<?xml version="1.0" encoding="utf-8" standalone="no"?>
<map>
 <!-- マップ名 ※必須※ -->
 <name>アリのスミス</name>
 <!-- マップバージョン ※必須※ -->
 <version>1.0.0</version>
<!-- 製作者  ※必須※　複数人指定可能 -->
 <authors>
    <author>suzukizoku</author>
 </authors>
<!-- ゲーム時間 半角数字で設定/分単位/
   未設定の場合25分に設定されます -->
 <time>25</time>
<!-- サブタイトルを設定します -->
<subtitle>族長の棲むアリのスミス</subtitle>
<!-- ゲーム開始時/途中参加時に配られる
アイテムの個数 半角数字で設定 未設定の場合5個に設定されます-->
 <start>
      <egg>7</egg>
      <bone>7</bone>
      <feather>7</feather>
 </start>
 
 <!-- 復活時に配られるアイテムの個数 
 半角数字で設定 未設定の場合5個に設定されます-->
 <rejail>
      <egg>4</egg>
      <bone>4</bone>
      <feather>4</feather>
 </rejail>
 
<!-- ゲーム自動終了  true/falseでの記述 -->
 <auto_end>true</auto_end>
<!-- 自動復活の初期値  true/falseでの記述 -->
 <auto_rejail>false</auto_rejail>
<!-- 観戦機能の初期値  true/falseでの記述 -->
 <watch>false</watch>
<!-- スポーンを許可する友好モブ 複数指定可能-->
 <mob_spawn>
        <type>POLAR_BEAR</type>
        <type>VILLAGER</type>
 </mob_spawn>
  <!-- OPG終了時のTP  true/falseでの記述 
   trueでテレポートを行う-->
 <opg_tp>false</opg_tp>
<location>
  <hunter>-260.5,11,-1800.5,180,0</hunter>
  <door>-260.5,11,-1800.5,180,0</door>
  <player>-234.5,11,-1819.5,0,0</player>
  <opg>-234.5,11,-1819.5,0,0</opg>
  <jail>-211.5,13.5,-1821.5,0,0</jail>
  <clear>-221.5,13.5,-1821.5,0,0</clear>
  <rejail1>-272.5,11,-1804.5,-90,0</rejail1>
  <rejail2>-220.5,57,-1804.5,180,0</rejail2>
  </location>
</map>
