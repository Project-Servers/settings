<?xml version="1.0" encoding="utf-8" standalone="no"?>
<map>
 <!-- マップ名 ※必須※ -->
 <name>おニューな鈴木ビル</name>
 <!-- マップバージョン ※必須※ -->
 <version>2.0.0</version>
 <desc>シロクマさんが棲むおニューな鈴木ビル,アップデートにより、リフトとそれに対応した通路が設置され,逃走に可能性をもたらす。,推奨ハンター数1～2人</desc>
<!-- サブタイトルを設定します -->
<subtitle>シロいクマァがおスキなビル</subtitle>
<!-- 製作者  ※必須※　複数人指定可能 -->
 <authors type = "arrange">
    <author type = "original">suzukizoku</author>
    <author>Yussan</author>
 </authors>
<!-- ゲーム時間 半角数字で設定/分単位/
   未設定の場合25分に設定されます -->
 <time>25</time>
<!-- ゲーム開始時/途中参加時に配られる
アイテムの個数 半角数字で設定 未設定の場合5個に設定されます-->
 <start>
      <egg>9</egg>
      <bone>11</bone>
      <feather>10</feather>
 </start>
 
 <!-- 復活時に配られるアイテムの個数 
 半角数字で設定 未設定の場合5個に設定されます-->
 <rejail>
      <egg>7</egg>
      <bone>9</bone>
      <feather>8</feather>
 </rejail>
 
<!-- ゲーム自動終了  true/falseでの記述 -->
 <auto_end>true</auto_end>
<!-- 自動復活の初期値  true/falseでの記述 -->
 <auto_rejail>false</auto_rejail>
<!-- 観戦機能の初期値  true/falseでの記述 -->
 <watch>false</watch>
<!-- スポーンを許可する友好モブ 複数指定可能-->
 <mob_spawn>
        <type>POLAR_BEAR</type>
        <type>VILLAGER</type>
        <type>SALMON</type>
 </mob_spawn>
  <!-- OPG終了時のTP  true/falseでの記述 
   trueでテレポートを行う-->
 <opg_tp>false</opg_tp>
<location>
  <hunter>1092.5,5.0,-1860.47,89.63,9.53</hunter>
  <door>1092.5,5.0,-1860.47,89.63,9.53</door>
  <player>1077.79,5.0,-1857.24,259.82,5.23</player>
  <opg>1077.79,5.0,-1857.24,259.82,5.23</opg>
  <jail>1015.43,7.5,-1817.35,270.27,32.87</jail>
  <clear>1014.76,6.5,-1805.48,268.42,40.86</clear>
  <rejail1>1060.8,83.0,-1803.45,181.18,16.29</rejail1>
  <rejail2>1086.96,29.0,-1832.84,89.02,13.83</rejail2>
   <!-- shopは設定しなくても可-->
   <shop>1080.52,83.0,-1839.38,315.16,2.14</shop>
</location>
</map>
