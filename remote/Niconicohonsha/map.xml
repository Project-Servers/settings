<?xml version="1.0" encoding="utf-8"?>
<map>
 <!-- マップ名 ※必須※ -->
 <name>ニコニコ本社(池袋PARCO)</name>
 <!-- マップバージョン ※必須※ -->
 <version>1.0.0</version>
<!-- 製作者  ※必須※　複数人指定可能 -->
 <authors>
    <author>Yussan</author>
 </authors>
<!-- ゲーム時間 半角数字で設定/分単位/
   未設定の場合25分に設定されます -->
 <time>25</time>
<!-- サブタイトルを設定します -->
<subtitle>ニコニコ本社で逃走中！</subtitle>
<!-- ゲーム開始時/途中参加時に配られる
アイテムの個数 半角数字で設定 未設定の場合5個に設定されます-->
 <start>
      <egg>8</egg>
      <bone>8</bone>
      <feather>8</feather>
 </start>
 
 <!-- 復活時に配られるアイテムの個数 
 半角数字で設定 未設定の場合5個に設定されます-->
 <rejail>
      <egg>5</egg>
      <bone>5</bone>
      <feather>5</feather>
 </rejail>
 
<!-- ゲーム自動終了  true/falseでの記述 -->
 <auto_end>true</auto_end>
<!-- 自動復活の初期値  true/falseでの記述 -->
 <auto_rejail>false</auto_rejail>
<!-- 観戦機能の初期値  true/falseでの記述 -->
 <watch>false</watch>
 <location>
     <player>96.5,7,-60.5,180,0</player>
	 <hunter>96.5,8,-67.5,0,0</hunter>
     <clear>15.5,23.5,-1.5,0,0</clear>
     <jail>15.5,17.5,-1.5,0,0</jail>
     <rejail1>224.5,6.5,-48.5,0,0</rejail1>
     <rejail2>71.5,7,-81.5,0,0</rejail2>
     <door>96.5,8,-67.5,0,0</door>
     <opg>96.5,7,-60.5,180,0</opg>
</location>
<!-- スポーンを許可する友好モブ 複数指定可能-->
 <mob_spawn>
        <type>POLAR_BEAR</type>
        <type>VILLAGER</type>
 </mob_spawn>
  <!-- OPG終了時のTP  true/falseでの記述 
   trueでテレポートを行う-->
 <opg_tp>false</opg_tp>
</map>