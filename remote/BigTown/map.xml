  <rejail2>-41.44,59.0,19.58,-90.55,-2.27</rejail2>
  <door>120.51,59.0,-58.45,-0.23,0.43</door>
  <opg>120.62,59.0,-49.2,-180.08,0.73</opg>
  </location>
</map>
