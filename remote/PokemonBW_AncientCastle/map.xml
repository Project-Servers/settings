<?xml version="1.0" encoding="utf-8" standalone="no"?><map>
 <!-- マップ名 ※必須※ -->
 <name>こだいのしろ～こだいのぬけみち</name>
 <!-- マップ説明 ,(コンマ)で改行 -->
 <desc>ポケットモンスター§7ブラック§fホワイト、,§rにて出てくるこだいのしろと,ポケットモンスター§7ブラック§92§fホワイト§c2,§rにて出てくるこだいのぬけみちです。,ウルガモスは居ませんよ。,§4推奨ハンター数:全エリア使用時2～3体</desc>
 <!-- マップバージョン ※必須※ -->
 <version>2.0.0</version>
<!-- 製作者  ※必須※　複数人指定可能 -->
 <authors>
    <author>creamhunter1106</author>
 </authors>
<!-- ゲーム時間 半角数字で設定/分単位/
   未設定の場合25分に設定されます -->
 <time>25</time>
<!-- サブタイトルを設定します -->
<subtitle>ポケモンの遺跡で逃走中!</subtitle>
<!-- ゲーム開始時/途中参加時に配られる
アイテムの個数 半角数字で設定 未設定の場合5個に設定されます-->
 <start>
      <egg>8</egg>
      <bone>10</bone>
      <feather>10</feather>
 </start>
 
 <!-- 復活時に配られるアイテムの個数 
 半角数字で設定 未設定の場合5個に設定されます-->
 <rejail>
      <egg>6</egg>
      <bone>7</bone>
      <feather>7</feather>
 </rejail>
 
   <!-- 各種地点 ※必須※-->
 <location>
      <player>607.50,56.0,498.50,90.0,0.1</player>
      <clear>551.50,62.5,629.50,-90.0,0.1</clear>
      <hunter>571.50,1.5,576.50,-90.0,0.1</hunter>
      <jail>594.50,55.5,560.50,-90.0,0.1</jail>
      <rejail1>572.50,19.0,630.50,179.9,0.1</rejail1>
      <rejail2>572.50,19.0,523.50,0.1,0.1</rejail2>
      <door>572.50,1.0,576.50,-90.0,0.1</door>
      <opg>582.50,1.5,576.50,90.0,0.1</opg>
      <!-- shopは設定しなくても可-->
      <shop>581.48,6.0,576.5,-89.87,0.0</shop>
 </location>
 
<!-- ゲーム自動終了  true/falseでの記述 -->
 <auto_end>false</auto_end>
<!-- 自動復活の初期値  true/falseでの記述 -->
 <auto_rejail>false</auto_rejail>
<!-- 観戦機能の初期値  true/falseでの記述 -->
 <watch>false</watch>
<!-- 天候の初期値  半角英数字で設定 0でnormal 1でhot 2でcold  
  指定以外の数値は強制的にnormalになります -->
 <climate>1</climate>
<!-- スポーンを許可する友好モブ 複数指定可能-->
 <mob_spawn>
        <type>POLAR_BEAR</type>
 </mob_spawn>
 <!-- OPG終了時のTP  true/falseでの記述 
   trueでテレポートを行う-->
 <opg_tp>false</opg_tp>
 <!-- 復活可能回数 -->
<rejailcount>10</rejailcount>
<!-- 特殊ブロック 半角数字で設定 0不運/1盲目/2発行/3加速 -->
<spblock>1</spblock>
</map>