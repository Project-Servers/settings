<?xml version="1.0" encoding="utf-8" standalone="no"?><map>
 <!-- マップ名 ※必須※ -->
 <name>ペットボトルの蓋</name>
 <!-- マップ説明 ,(コンマ)で改行 -->
 <desc>午後の紅茶レモンティーの蓋です。,美味しいよね。,推奨ハンター数:1体</desc>
 <!-- マップバージョン ※必須※ -->
 <version>1.1.0</version>
<!-- 製作者  ※必須※　複数人指定可能 -->
 <authors>
    <author>creamhunter1106</author>
 </authors>
<!-- ゲーム時間 半角数字で設定/分単位/
   未設定の場合25分に設定されます -->
 <time>25</time>
<!-- サブタイトルを設定します -->
<subtitle>蓋の中で逃走中!</subtitle>
<!-- ゲーム開始時/途中参加時に配られる
アイテムの個数 半角数字で設定 未設定の場合5個に設定されます-->
 <start>
      <egg>8</egg>
      <bone>10</bone>
      <feather>10</feather>
 </start>
 
 <!-- 復活時に配られるアイテムの個数 
 半角数字で設定 未設定の場合5個に設定されます-->
 <rejail>
      <egg>6</egg>
      <bone>7</bone>
      <feather>7</feather>
 </rejail>
 
   <!-- 各種地点 ※必須※-->
 <location>
      <player>453.50,6.0,-605.50,-90.0,0.1</player>
      <clear>499.50,5.5,-609.50,90.0,0.1</clear>
      <hunter>493.50,4.0,-605.50,90.0,0.1</hunter>
      <jail>499.50,5.5,-602.50,90.0,0.1</jail>
      <rejail1>453.50,4.0,-641.50,0.1,0.1</rejail1>
      <rejail2>453.50,4.0,-569.50,179.9,0.1</rejail2>
      <door>492.50,4.0,-605.50,90.0,0.1</door>
      <opg>486.50,4.0,-605.50,-90.0,0.1</opg>
      <!-- shopは設定しなくても可-->
      <shop>412.69,4.0,-605.51,270.29,0.15</shop>
 </location>
 
<!-- ゲーム自動終了  true/falseでの記述 -->
 <auto_end>false</auto_end>
<!-- 自動復活の初期値  true/falseでの記述 -->
 <auto_rejail>false</auto_rejail>
<!-- 観戦機能の初期値  true/falseでの記述 -->
 <watch>false</watch>
<!-- スポーンを許可する友好モブ 複数指定可能-->
 <mob_spawn>
        <type>POLAR_BEAR</type>
 </mob_spawn>
 <!-- OPG終了時のTP  true/falseでの記述 
   trueでテレポートを行う-->
 <opg_tp>false</opg_tp>
 <!-- 復活可能回数 -->
<rejailcount>10</rejailcount>
<!-- 特殊ブロック 半角数字で設定 0不運/1盲目/2発行/3加速 -->
<spblock>1</spblock>
</map>