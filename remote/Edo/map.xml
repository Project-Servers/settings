<?xml version="1.0" encoding="utf-8" standalone="no"?>
<map>
 <!-- マップ名 ※必須※ -->
 <name>江戸の町</name>
 <!-- マップバージョン ※必須※ -->
 <version>1.1.1</version>
<!-- 製作者  ※必須※　複数人指定可能 -->
 <authors>
    <author>Yussan</author>
 </authors>
<!-- ゲーム時間 半角数字で設定/分単位/
   未設定の場合25分に設定されます -->
 <time>25</time>
<!-- サブタイトルを設定します -->
<subtitle>江戸の町</subtitle>
<!-- ゲーム開始時/途中参加時に配られる
アイテムの個数 半角数字で設定 未設定の場合5個に設定されます-->
 <start>
      <egg>10</egg>
      <bone>10</bone>
      <feather>10</feather>
 </start>
 
 <!-- 復活時に配られるアイテムの個数 
 半角数字で設定 未設定の場合5個に設定されます-->
 <rejail>
      <egg>10</egg>
      <bone>10</bone>
      <feather>10</feather>
 </rejail>
 
<!-- ゲーム自動終了  true/falseでの記述 -->
 <auto_end>true</auto_end>
<!-- 自動復活の初期値  true/falseでの記述 -->
 <auto_rejail>false</auto_rejail>
<!-- 観戦機能の初期値  true/falseでの記述 -->
 <watch>false</watch>
<!-- スポーンを許可する友好モブ 複数指定可能-->
 <mob_spawn>
        <type>POLAR_BEAR</type>
        <type>VILLAGER</type>
 </mob_spawn>
  <!-- OPG終了時のTP  true/falseでの記述 
   trueでテレポートを行う-->
 <opg_tp>false</opg_tp>
<location>
    <hunter>-17.6,45.5,186.08,-0.9,1.5</hunter>
  <door>-17.6,45.5,187.70,-0.9,1.5</door>
  <player>-17.5,44.5,195.48,179.1,0.3</player>
  <opg>-17.5,44.5,195.48,179.1,0.3</opg>
  <jail>-67.72,47.0,182.42,161.9,13.2</jail>
  <clear>-40.61,47.0,182.28,163.8,11.4</clear>
  <rejail1>29.67,44.5,45.19,89.4,-0.9</rejail1>
  <rejail2>-29.67,44.5,221.86,-90.0,0.3</rejail2>
  </location>
</map>
