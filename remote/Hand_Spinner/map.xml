<?xml version="1.0" encoding="utf-8" standalone="no"?><map>
 <!-- マップ名 ※必須※ -->
 <name>ハンドスピナー</name>
 <!-- マップ説明 ,(コンマ)で改行 -->
 <desc>ハンドスピナーを元にしたステージ。,正直それ以外言うことがない。,§4推奨ハンター数:1体</desc>
 <!-- マップバージョン ※必須※ -->
 <version>1.0.0</version>
<!-- 製作者  ※必須※　複数人指定可能 -->
 <authors>
    <author>creamhunter1106</author>
 </authors>
<!-- ゲーム時間 半角数字で設定/分単位/
   未設定の場合25分に設定されます -->
 <time>25</time>
<!-- サブタイトルを設定します -->
<subtitle>最近全く見ないよね…うん…</subtitle>
<!-- ゲーム開始時/途中参加時に配られる
アイテムの個数 半角数字で設定 未設定の場合5個に設定されます-->
 <start>
      <egg>8</egg>
      <bone>10</bone>
      <feather>10</feather>
 </start>
 
 <!-- 復活時に配られるアイテムの個数 
 半角数字で設定 未設定の場合5個に設定されます-->
 <rejail>
      <egg>6</egg>
      <bone>7</bone>
      <feather>7</feather>
 </rejail>
 
   <!-- 各種地点 ※必須※-->
 <location>
      <player>7.50,19.0,8.50,-90.0,0.0</player>
      <clear>34.50,26.5,5.50,90.0,0.0</clear>
      <hunter>8.50,27.0,8.50,90.0,0.0</hunter>
      <jail>34.50,26.5,11.50,90.0,0.0</jail>
      <rejail1>39.50,21.0,-39.50,45.0,0.0</rejail1>
      <rejail2>39.50,21.0,55.50,135.0,0.0</rejail2>
      <door>7.50,27.0,8.50,90.0,0.0</door>
      <opg>0.50,27.0,8.50,-90.0,0.0</opg>
      <!-- shopは設定しなくても可-->
      <shop>29.48,25.0,8.49,90.06,0.44</shop>
 </location>
 
<!-- ゲーム自動終了  true/falseでの記述 -->
 <auto_end>false</auto_end>
<!-- 自動復活の初期値  true/falseでの記述 -->
 <auto_rejail>false</auto_rejail>
<!-- 観戦機能の初期値  true/falseでの記述 -->
 <watch>false</watch>
<!-- スポーンを許可する友好モブ 複数指定可能-->
 <mob_spawn>
        <type>POLAR_BEAR</type>
 </mob_spawn>
 <!-- OPG終了時のTP  true/falseでの記述 
   trueでテレポートを行う-->
 <opg_tp>false</opg_tp>
 <!-- 復活可能回数 -->
<rejailcount>10</rejailcount>
<!-- 特殊ブロック 半角数字で設定 0不運/1盲目/2発行/3加速 -->
<spblock>1</spblock>
</map>