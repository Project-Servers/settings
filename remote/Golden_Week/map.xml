<?xml version="1.0" encoding="utf-8" standalone="no"?><map>
 <!-- マップ名 ※必須※ -->
 <name>Golden Week!!</name>
 <!-- マップバージョン ※必須※ -->
 <version>3.0.1</version>
<!-- 製作者  ※必須※　複数人指定可能 -->
 <authors>
    <author>Takashing_1185</author>
    <author>inazuma0415</author>
 </authors>
<!-- サブタイトルを設定します -->
<subtitle>コロナを許すな。</subtitle>

<!-- ゲーム時間 半角数字で設定/分単位/
   未設定の場合25分に設定されます -->
 <time>25</time>

<!-- ゲーム開始時/途中参加時に配られる
アイテムの個数 半角数字で設定 未設定の場合5個に設定されます-->
 <start>
      <egg>12</egg>
      <bone>10</bone>
      <feather>12</feather>
 </start>
 <!-- 復活時に配られるアイテムの個数 
 半角数字で設定 未設定の場合5個に設定されます-->
 <rejail>
      <egg>8</egg>
      <bone>8</bone>
      <feather>8</feather>
 </rejail>
 <!-- 各種地点 ※必須※-->
 <location>
      <player>161.52,12.0,-338.52,-269.73,0.56</player>
      <clear>289.4,40.5,-239.46,-177.69,63.72</clear>
      <hunter>139.3,12.0,-338.47,-90.55,1.59</hunter>
      <jail>318.5,40.5,-265.5,-359.73,0.28</jail>
      <rejail1>334.47,16.0,-143.78,-180.32,-0.59</rejail1>
      <rejail2>151.73,13.0,-226.51,-90.19,-0.59</rejail2>
      <door>139.3,12.0,-338.47,-90.55,1.59</door>
      <opg>161.52,12.0,-338.52,-269.73,0.56</opg>
      <shop>448.51,14.0,-336.52,-270.01,2.09</shop>
 </location>
 <!-- 特殊ブロック 半角数字で設定 0不運/1盲目/2発行/3加速 -->
 <spblock>2</spblock>
 <desc>§b大学をマップにしました。,§b休学にさせたコロナを俺は絶対に許さない。,§c推奨ハンター数2-3人</desc>

<!-- スポーンを許可する友好モブ 複数指定可能-->
 <mob_spawn>
        <type>VILLAGER</type>
 </mob_spawn>

</map>
