<?xml version="1.0" encoding="utf-8" standalone="no"?>
<map>
 <!-- マップ名 ※必須※ -->
 <name>マイクラ逃走中専用ステージ エリア02</name>
 <!-- マップバージョン ※必須※ -->
 <version>1.0.0</version>
<!-- サブタイトルを設定します -->
<subtitle>逃走中専用エリアで逃走中！</subtitle>
<!-- 製作者  ※必須※　複数人指定可能 -->
 <authors>
    <author>Light</author>
 </authors>
<!-- ゲーム時間 半角数字で設定/分単位/
   未設定の場合25分に設定されます -->
 <time>25</time>
<!-- ゲーム開始時/途中参加時に配られる
アイテムの個数 半角数字で設定 未設定の場合5個に設定されます-->
 <start>
      <egg>8</egg>
      <bone>8</bone>
      <feather>8</feather>
 </start>
 
 <!-- 復活時に配られるアイテムの個数 
 半角数字で設定 未設定の場合5個に設定されます-->
 <rejail>
      <egg>6</egg>
      <bone>6</bone>
      <feather>6</feather>
 </rejail>
 
<!-- ゲーム自動終了  true/falseでの記述 -->
 <auto_end>true</auto_end>
<!-- 自動復活の初期値  true/falseでの記述 -->
 <auto_rejail>false</auto_rejail>
<!-- 観戦機能の初期値  true/falseでの記述 -->
 <watch>false</watch>
<!-- 気候の初期値  半角英数字で設定 0でnormal 1でhot 2でcold  
  指定以外の数値は強制的にnormalになります -->
 <climate>0</climate>
<!-- スポーンを許可する友好モブ 複数指定可能-->
 <mob_spawn>
        <type>POLAR_BEAR</type>
        <type>VILLAGER</type>
 </mob_spawn>
  <!-- OPG終了時のTP  true/falseでの記述 
   trueでテレポートを行う-->
 <opg_tp>false</opg_tp>
<location>
  <player>510.5,4,72.5,-90,0</player>
  <hunter>516.5,5,72.5,90,0</hunter>
  <door>516.5,5,72.5,90,0</door>
  <clear>432.5,6.5,137.5,180,0</clear>
  <jail>442.5,7.5,137.5,180,0</jail>
  <rejail1>587.5,4,42.5,180,0</rejail1>
  <rejail2>456.5,11,169.5,180,0</rejail2>
  <opg>510.5,4,72.5,-90,0</opg>
</location>
</map>
