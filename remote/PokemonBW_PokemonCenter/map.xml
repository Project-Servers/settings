<?xml version="1.0" encoding="utf-8" standalone="no"?><map>
 <!-- マップ名 ※必須※ -->
 <name>ポケモンセンター(BW)</name>
 <!-- マップ説明 ,(コンマ)で改行 -->
 <desc>ポケットモンスター§7ブラック§fホワイト,§rにて出てくるポケモンセンターです。,外には出れませんのでご注意を。,§4推奨ハンター数:1体</desc>
 <!-- マップバージョン ※必須※ -->
 <version>1.1.0</version>
<!-- 製作者  ※必須※　複数人指定可能 -->
 <authors>
    <author>creamhunter1106</author>
 </authors>
<!-- ゲーム時間 半角数字で設定/分単位/
   未設定の場合25分に設定されます -->
 <time>25</time>
<!-- サブタイトルを設定します -->
<subtitle>げんきのかけらをお買い求めください。</subtitle>
<!-- ゲーム開始時/途中参加時に配られる
アイテムの個数 半角数字で設定 未設定の場合5個に設定されます-->
 <start>
      <egg>8</egg>
      <bone>10</bone>
      <feather>10</feather>
 </start>
 
 <!-- 復活時に配られるアイテムの個数 
 半角数字で設定 未設定の場合5個に設定されます-->
 <rejail>
      <egg>6</egg>
      <bone>7</bone>
      <feather>7</feather>
 </rejail>
 
   <!-- 各種地点 ※必須※-->
 <location>
      <player>-711.50,4.0,447.50,-90.0,0.1</player>
      <clear>-621.50,25.5,462.50,90.0,0.1</clear>
      <hunter>-670.50,4.0,447.50,90.0,0.1</hunter>
      <jail>-621.50,25.5,432.50,90.0,0.1</jail>
      <rejail1>-652.50,7.0625,427.50,45.0,0.1</rejail1>
      <rejail2>-652.50,7.0625,467.50,135.0,0.1</rejail2>
      <door>-671.50,4.0,447.50,90.0,0.1</door>
      <opg>-679.50,4.0,447.50,-90.0,0.1</opg>
      <!-- shopは設定しなくても可-->
      <shop>-696.47,7.0,462.3,180.33,0.15</shop>
 </location>
 
<!-- ゲーム自動終了  true/falseでの記述 -->
 <auto_end>false</auto_end>
<!-- 自動復活の初期値  true/falseでの記述 -->
 <auto_rejail>false</auto_rejail>
<!-- 観戦機能の初期値  true/falseでの記述 -->
 <watch>false</watch>
<!-- 天候の初期値  半角英数字で設定 0でnormal 1でhot 2でcold  
  指定以外の数値は強制的にnormalになります -->
 <climate>0</climate>
<!-- スポーンを許可する友好モブ 複数指定可能-->
 <mob_spawn>
        <type>POLAR_BEAR</type>
 </mob_spawn>
 <!-- OPG終了時のTP  true/falseでの記述 
   trueでテレポートを行う-->
 <opg_tp>false</opg_tp>
 <!-- 復活可能回数 -->
<rejailcount>10</rejailcount>
<!-- 特殊ブロック 半角数字で設定 0不運/1盲目/2発行/3加速 -->
<spblock>1</spblock>
</map>