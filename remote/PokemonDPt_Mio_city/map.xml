<time>30</time>
<!--  ゲーム開始時/途中参加時に配られる
アイテムの個数 半角数字で設定 未設定の場合5個に設定されます -->
<start>
<egg>10</egg>
<bone>10</bone>
<feather>12</feather>
</start>
<!--  復活時に配られるアイテムの個数 
 半角数字で設定 未設定の場合5個に設定されます -->
<rejail>
<egg>8</egg>
<bone>8</bone>
<feather>9</feather>
</rejail>
<!-- 各種地点 ※必須※-->
<location>
     <player>-11.52,12.0,-39.59,-180.46,21.9</player>
     <clear>44.45,14.5,-55.65,-1.49,5.25</clear>
