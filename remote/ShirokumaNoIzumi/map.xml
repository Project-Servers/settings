<map>
<!--  マップ名 ※必須※  -->
<name>白熊の泉</name>
<!--  マップバージョン ※必須※  -->
<version>1.0.0</version>
<!-- サブタイトルを設定します -->
<subtitle>白熊の泉で逃走中！</subtitle>
<!--  製作者  ※必須※　複数人指定可能  -->
<authors>
<author>suzukizoku</author>
</authors>
<!--  ゲーム時間 半角数字で設定/分単位/
   未設定の場合25分に設定されます  -->
<time>25</time>
<!--  ゲーム開始時/途中参加時に配られる
アイテムの個数 半角数字で設定 未設定の場合5個に設定されます -->
<start>
<egg>8</egg>
<bone>8</bone>
<feather>8</feather>
</start>
<!--  復活時に配られるアイテムの個数 
 半角数字で設定 未設定の場合5個に設定されます -->
<rejail>
<egg>6</egg>
<bone>6</bone>
<feather>6</feather>
</rejail>
<!-- 各種地点 ※必須※-->
<location>
     <player>8.58,4.0,5.76,179.4,-0.61</player>
     <clear>7.04,38.5,-27.43,-3.08,14.71</clear>
     <hunter>8.52,4.0,-5.3,358.8,-5.52</hunter>
     <jail>-4.36,39.5,-28.64,0.59,12.86</jail>
     <rejail1>29.45,33.0,8.52,92.75,1.19</rejail1>
     <rejail2>-0.18,9.0,21.14,149.28,6.1</rejail2>
     <door>8.52,4.0,-4.87,358.8,-5.52</door>
     <opg>8.58,4.0,5.76,179.4,-0.61</opg>
</location>
<!--  ゲーム自動終了  true/falseでの記述  -->
<auto_end>true</auto_end>
<!--  自動復活の初期値  true/falseでの記述  -->
<auto_rejail>false</auto_rejail>
<!--  観戦機能の初期値  true/falseでの記述  -->
<watch>false</watch>
<!--  スポーンを許可する友好モブ 複数指定可能 -->
<mob_spawn>
<type>POLAR_BEAR</type>
<type>BAT</type>
<type>WOLF</type>
</mob_spawn>
</map>