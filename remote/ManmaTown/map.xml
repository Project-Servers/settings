<map>
<!--  マップ名 ※必須※  -->
<name>ManmaTown</name>
<!--  マップバージョン ※必須※  -->
<version>1.1.0</version>
<!--  製作者  ※必須※　複数人指定可能  -->
<authors>
<author>まんま</author>
</authors>
<!--  ゲーム時間 半角数字で設定/分単位/
   未設定の場合25分に設定されます  -->
<time>25</time>
<!-- サブタイトルを設定します -->
<subtitle>まんまさんの街</subtitle>
<!--  ゲーム開始時/途中参加時に配られる
アイテムの個数 半角数字で設定 未設定の場合5個に設定されます -->
<start>
<egg>6</egg>
<bone>6</bone>
<feather>6</feather>
</start>
<!--  復活時に配られるアイテムの個数 
 半角数字で設定 未設定の場合5個に設定されます -->
<rejail>
<egg>4</egg>
<bone>4</bone>
<feather>4</feather>
</rejail>
<!-- 各種地点 ※必須※-->
<location>
     <player>-56.59,56.5,531.30,179.8,0.3</player>
     <clear>-19.56,58.5,692.57,-89.1,3.1</clear>
     <hunter>-56.72,57.5,499.29,-2.1,2.1</hunter>
     <jail>-19.44,58.5,702.54,-92.2,-0.6</jail>
     <rejail1>58.08,56.5,849.65,131.3,0.9</rejail1>
     <rejail2>-56.69,56.5,756.59,-132.3,-1.5</rejail2>
     <door>-56.40,57.0,503.70,0.4,8.9</door>
     <opg>-56.59,56.5,531.30,179.8,0.3</opg>
</location>
<!--  ゲーム自動終了  true/falseでの記述  -->
<auto_end>true</auto_end>
<!--  自動復活の初期値  true/falseでの記述  -->
<auto_rejail>false</auto_rejail>
<!--  観戦機能の初期値  true/falseでの記述  -->
<watch>false</watch>
<!--  スポーンを許可する友好モブ 複数指定可能 -->
<mob_spawn>
<type>POLAR_BEAR</type>
<type>BAT</type>
<type>WOLF</type>
</mob_spawn>
<!-- 復活可能回数 -->
<rejailcount>7</rejailcount>
</map>