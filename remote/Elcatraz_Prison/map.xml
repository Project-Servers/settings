<map>
<!--  マップ名 ※必須※  -->
<name>イルカトラズ刑務所</name>
<!--  マップバージョン ※必須※  -->
<version>1.2.0</version>
<!-- サブタイトルを設定します -->
<subtitle>～脱獄する逃走者～</subtitle>
<desc>§b小さな刑務所,§bお風呂には適当な水路があります,§c推奨ハンター数:1</desc>
<authors>
<author>riku0745</author>
</authors>
<!--  ゲーム時間 半角数字で設定/分単位/
   未設定の場合25分に設定されます  -->
<time>25</time>
<!--  ゲーム開始時/途中参加時に配られる
アイテムの個数 半角数字で設定 未設定の場合5個に設定されます -->
<start>
<egg>10</egg>
<bone>10</bone>
<feather>10</feather>
</start>
<!--  復活時に配られるアイテムの個数 
 半角数字で設定 未設定の場合5個に設定されます -->
<rejail>
<egg>7</egg>
<bone>7</bone>
<feather>7</feather>
</rejail>
<!-- 各種地点 ※必須※-->
<location>
     <player>138.5,4.0,608.5,0.0,0.0</player>
     <clear>131.5,4.0,597.5,90.0,0.0</clear>
     <hunter>138.5,4.0,613.5,180.0,0.0</hunter>
     <jail>96.5,11.0,623.5,0.0,0.0</jail>
     <rejail1>77.5,4.0,608.5,-90.0,0.0</rejail1>
     <rejail2>185.5,4.0,608.5,90.0,0.0</rejail2>
     <door>138.5,4.0,612.5,180.0,0.0</door>
     <opg>138.5,4.0,608.5,0.0,0.0</opg>
	 <shop>131.5,4.0,642.5,180.0,0.0</shop>
</location>
<!--  ゲーム自動終了  true/falseでの記述  -->
<auto_end>true</auto_end>
<!--  自動復活の初期値  true/falseでの記述  -->
<auto_rejail>false</auto_rejail>
<!--  観戦機能の初期値  true/falseでの記述  -->
<watch>false</watch>
<!--
 天候の初期値  半角英数字で設定 0でnormal 1でhot 2でcold  
  指定以外の数値は強制的にnormalになります 
-->
<climate>0</climate>
<!--  スポーンを許可する友好モブ 複数指定可能 -->
<mob_spawn>
<type>POLAR_BEAR</type>
<type>BAT</type>
<type>WOLF</type>
</mob_spawn>
</map>