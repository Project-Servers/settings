<?xml version="1.0" encoding="utf-8" standalone="no"?>
<map>
 <!-- マップ名 ※必須※ -->
 <name>七かける七ルームズ</name>
 <!-- マップバージョン ※必須※ -->
 <version>2.0.0</version>
<!-- サブタイトルを設定します -->
<subtitle>7×7=49Rooms</subtitle>
<desc>おニューな7×7Roomsに進化！,縦と横の扉は時間ごとに開閉する。,真ん中の金ブロックの上にクマちゃんがいるよ！,推奨ハンター:1体</desc>
<!-- 製作者  ※必須※　複数人指定可能 -->
 <authors type = "arrange">
    <author type = "original">suzukizoku</author>
    <author>Yussan</author>
 </authors>
<!-- ゲーム時間 半角数字で設定/分単位/
   未設定の場合25分に設定されます -->
 <time>25</time>
<!-- ゲーム開始時/途中参加時に配られる
アイテムの個数 半角数字で設定 未設定の場合5個に設定されます-->
 <start>
      <egg>9</egg>
      <bone>11</bone>
      <feather>10</feather>
 </start>
 
 <!-- 復活時に配られるアイテムの個数 
 半角数字で設定 未設定の場合5個に設定されます-->
 <rejail>
      <egg>7</egg>
      <bone>9</bone>
      <feather>8</feather>
 </rejail>
 
<!-- ゲーム自動終了  true/falseでの記述 -->
 <auto_end>true</auto_end>
<!-- 自動復活の初期値  true/falseでの記述 -->
 <auto_rejail>false</auto_rejail>
<!-- 観戦機能の初期値  true/falseでの記述 -->
 <watch>false</watch>
<!-- スポーンを許可する友好モブ 複数指定可能-->
 <mob_spawn>
        <type>POLAR_BEAR</type>
        <type>VILLAGER</type>
 </mob_spawn>
  <!-- OPG終了時のTP  true/falseでの記述 
   trueでテレポートを行う-->
 <opg_tp>false</opg_tp>
<location>
  <hunter>68.4,12.0,26.55,90.35,2.14</hunter>
  <door>68.4,12.0,26.55,90.35,2.14</door>
  <player>60.43,12.0,26.53,269.14,0.29</player>
  <opg>60.43,12.0,26.53,269.14,0.29</opg>
  <jail>16.56,14.5,-10.54,-1.22,59.28</jail>
  <clear>6.4,13.5,-11.71,-1.22,59.89</clear>
  <rejail1>26.06,12.0,26.59,90.31,14.42</rejail1>
  <rejail2>2.19,12.0,50.78,221.79,7.67</rejail2>
  <shop>1.89,12.0,51.02,-133.82,7.05</shop>
  </location>
</map>
