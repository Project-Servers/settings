<?xml version="1.0" encoding="utf-8" standalone="no"?>
<map>
	<!-- マップ名 ※必須※ -->
	<name>巨大振り子時計</name>
	<!-- マップバージョン ※必須※ -->
	<version>1.0.0</version>
	<!-- 製作者  ※必須※　複数人指定可能 -->
	<authors>
		<author>Syuryu0919</author>
	</authors>
	<!-- ゲーム時間 半角数字で設定/分単位/
 ? 未設定の場合25分に設定されます -->
	<time>25</time>
	<!-- サブタイトルを設定します -->
	<subtitle>今は、もう、動かない</subtitle>
	<!-- ゲーム開始時/途中参加時に配られる
アイテムの個数 半角数字で設定 未設定の場合5個に設定されます-->
	<start>
		<egg>10</egg>
		<bone>10</bone>
		<feather>10</feather>
	</start>
	<!-- 復活時に配られるアイテムの個数 
 半角数字で設定 未設定の場合5個に設定されます-->
	<rejail>
		<egg>8</egg>
		<bone>8</bone>
		<feather>8</feather>
	</rejail>
	<!-- 各種地点 ※必須※-->
	<location>
		<player>790.5,53.0,-403.5,0.0,0.0</player>
		<clear>790.5,53.0,-362.5,180.0,0.0</clear>
		<hunter>805.5,50.0,-403.5,90.0,0.0</hunter>
		<jail>790.5,22.0,-403.5,0.0,0.0</jail>
		<rejail1>790.5,50.0,-422.5,0.0,0.0</rejail1>
		<rejail2>790.5,5.0,-480.5,0.0,0.0</rejail2>
		<door>804.5,50.0,-403.5,0.0,0.0</door>
		<opg>796.5,50.0,-403.5,270.0,0.0</opg>
	</location>
	<!-- ゲーム自動終了  true/falseでの記述 -->
	<auto_end>true</auto_end>
	<!-- 自動復活の初期値  true/falseでの記述 -->
	<auto_rejail>true</auto_rejail>
	<!-- 観戦機能の初期値  true/falseでの記述 -->
	<watch>false</watch>
	<!-- スポーンを許可する友好モブ 複数指定可能-->
	<mob_spawn>
		<type>POLAR_BEAR</type>
		<type>BAT</type>
	</mob_spawn>
	<!-- OPG終了時のTP  true/falseでの記述 
   trueでテレポートを行う-->
	<opg_tp>false</opg_tp>
	<!-- 復活可能回数 -->
	<rejailcount>10</rejailcount>
	<!-- 特殊ブロック 半角数字で設定 0不運/1盲目/2発行/3加速 -->
	<spblock>2</spblock>
	<desc>誰が何のために作ったのか,わからない、遠くからでも,よく見える時計で今度こそ逃げ回る！,§l§b現在のバージョン1.0.1,§4推奨ハンター数1～2体§r</desc>
</map>
