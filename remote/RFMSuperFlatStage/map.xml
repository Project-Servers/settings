<map>
<!--  マップ名 ※必須※  -->
<name>RFMSuperFlatStage</name>
<!--  マップバージョン ※必須※  -->
<version>1.0.0</version>
<!-- サブタイトルを設定します -->
<subtitle>スーパーフラットステージで逃走中！</subtitle>
<!--  製作者  ※必須※　複数人指定可能  -->
<authors>
<author>Yussan</author>
</authors>
<!--  ゲーム時間 半角数字で設定/分単位/
   未設定の場合25分に設定されます  -->
<time>25</time>
<!--  ゲーム開始時/途中参加時に配られる
アイテムの個数 半角数字で設定 未設定の場合5個に設定されます -->
<start>
<egg>8</egg>
<bone>8</bone>
<feather>8</feather>
</start>
<!--  復活時に配られるアイテムの個数 
 半角数字で設定 未設定の場合5個に設定されます -->
<rejail>
<egg>6</egg>
<bone>6</bone>
<feather>6</feather>
</rejail>
<!-- 各種地点 ※必須※-->
<location>
     <player>-387.5,4.0625,-1566.5,90,0</player>
	 <hunter>-408.5,4,-1566.5,-90,0</hunter>
     <clear>-404.5,5.5,-1641.5,0,0</clear>
     <jail>-404.5,6.5,-1626.5,0,0</jail>
     <rejail1>-343.0,4,-1547.0,0,0</rejail1>
     <rejail2>-343.0,4,-1547.0,0,0</rejail2>
     <door>-408.5,4,-1566.5,-90,0</door>
     <opg>-387.5,4.0625,-1566.5,90,0</opg>
</location>
<!--  ゲーム自動終了  true/falseでの記述  -->
<auto_end>true</auto_end>
<!--  自動復活の初期値  true/falseでの記述  -->
<auto_rejail>false</auto_rejail>
<!--  観戦機能の初期値  true/falseでの記述  -->
<watch>false</watch>
<!--  スポーンを許可する友好モブ 複数指定可能 -->
<mob_spawn>
<type>POLAR_BEAR</type>
<type>BAT</type>
<type>WOLF</type>
</mob_spawn>
</map>