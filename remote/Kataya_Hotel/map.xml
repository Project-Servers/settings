<?xml version="1.0" encoding="utf-8" standalone="no"?>
<map>
 <!-- マップ名 ※必須※ -->
 <name>カタヤホテル</name>
 <!-- マップバージョン ※必須※ -->
 <version>1.0.0</version>
<!-- 製作者  ※必須※　複数人指定可能 -->
 <authors>
    <author>カタヤ</author>
 </authors>
<!-- ゲーム時間 半角数字で設定/分単位/
   未設定の場合25分に設定されます -->
 <time>25</time>
<!-- サブタイトルを設定します -->
<subtitle>カタヤホテルで逃走中！</subtitle>
<!-- ゲーム開始時/途中参加時に配られる
アイテムの個数 半角数字で設定 未設定の場合5個に設定されます-->
 <start>
      <egg>7</egg>
      <bone>7</bone>
      <feather>7</feather>
 </start>
 
 <!-- 復活時に配られるアイテムの個数 
 半角数字で設定 未設定の場合5個に設定されます-->
 <rejail>
      <egg>4</egg>
      <bone>4</bone>
      <feather>4</feather>
 </rejail>
 
<!-- ゲーム自動終了  true/falseでの記述 -->
 <auto_end>true</auto_end>
<!-- 自動復活の初期値  true/falseでの記述 -->
 <auto_rejail>false</auto_rejail>
<!-- 観戦機能の初期値  true/falseでの記述 -->
 <watch>false</watch>
<!-- スポーンを許可する友好モブ 複数指定可能-->
 <mob_spawn>
        <type>POLAR_BEAR</type>
        <type>VILLAGER</type>
 </mob_spawn>
  <!-- OPG終了時のTP  true/falseでの記述 
   trueでテレポートを行う-->
 <opg_tp>false</opg_tp>
<location>
    <hunter>961.41,4.0,455.44,-270.95,6.14</hunter>
  <door>960.48,4.0,455.46,-270.95,6.14</door>
  <player>977.2,5.5,437.54,279.55,12.9</player>
  <opg>956.27,4.0,455.16,-87.36,9.83</opg>
  <jail>940.29,5.5,465.48,-74.38,24.57</jail>
  <clear>940.34,5.5,476.38,-106.94,50.38</clear>
  <rejail1>977.2,5.5,437.54,279.55,12.9</rejail1>
  <rejail2>1008.65,33.0,469.67,-55.37,6.14</rejail2>
  </location>
</map>
