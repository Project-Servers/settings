<?xml version="1.0" encoding="utf-8" standalone="no"?><map>
 <!-- マップ名 ※必須※ -->
 <name>GTASA Area69</name>
 <!-- マップ説明 ,(コンマ)で改行 -->
 <desc></desc>
 <!-- マップバージョン ※必須※ -->
 <version>1.2.0</version>
<!-- 製作者  ※必須※　複数人指定可能 -->
 <authors type = "arrange">
     <author type= "original">PlanetMinecraft</author>
	 <author>Yussan</author>
</authors> 
<!-- ゲーム時間 半角数字で設定/分単位/
   未設定の場合25分に設定されます -->
 <time>25</time>
<!-- サブタイトルを設定します -->
<subtitle>Area69 Police Scanner</subtitle>
<!-- ゲーム開始時/途中参加時に配られる
アイテムの個数 半角数字で設定 未設定の場合5個に設定されます-->
 <start>
      <egg>12</egg>
      <bone>12</bone>
      <feather>12</feather>
 </start>
 <!-- 復活時に配られるアイテムの個数 
 半角数字で設定 未設定の場合5個に設定されます-->
 <rejail>
      <egg>12</egg>
      <bone>12</bone>
      <feather>12</feather>
 </rejail>
   <!-- 各種地点 ※必須※-->
 <location>            
      <player>-141.44,69.0,-132.57,-179.49,-2.16</player>
      <clear>-181.58,70.5,-75.22,-0.74,51.9</clear>     
	  <hunter>-141.46,70.0,-162.48,-0.09,0.29</hunter>
      <jail>-196.3,71.5,-75.36,-1.36,29.78</jail>
      <rejail1>-173.5,50.0,-96.63,-180.76,4.59</rejail1>
      <rejail2>-21.88,69.0,-109.28,45.94,2.14</rejail2>
      <door>-191.54,66.06,443.3,-180.63,12.9</door>
      <opg>-141.44,69.0,-132.57,-179.49,-2.16</opg>
      <!-- shopは設定しなくても可-->
      <shop>-151.48,62.0,-75.4,-38.22,7.05</shop>
 </location>
<!-- ゲーム自動終了  true/falseでの記述 -->
 <auto_end>true</auto_end>
<!-- 自動復活の初期値  true/falseでの記述 -->
 <auto_rejail>false</auto_rejail>
<!-- 観戦機能の初期値  true/falseでの記述 -->
 <watch>false</watch>
<!-- スポーンを許可する友好モブ 複数指定可能-->
 <mob_spawn>
        <type>POLAR_BEAR</type>
		<type>VILLAGER</type>
 </mob_spawn>
 <!-- OPG終了時のTP  true/falseでの記述 
   trueでテレポートを行う-->
 <opg_tp>false</opg_tp>
 <!-- 復活可能回数 -->
<rejailcount>10</rejailcount>
<!-- 特殊ブロック 半角数字で設定 0不運/1盲目/2発行/3加速 -->
<spblock>2</spblock>
<!-- 時間帯の初期値  半角英数字で設定 0/朝,1/昼,2/夜,  
  指定以外の数値や未設定の場合は0になります -->
 <timezone>1</timezone>
 <!-- フィールドの初期値  半角英数字で設定 
 0/平穏,1/温暖,2/寒冷,3/灼熱,4/極寒,5/絶望,6/希望,7/暴風,8/天雷,9/感染  
  指定以外の数値や未設定の場合は0になります -->
 <field>0</field>
</map>
