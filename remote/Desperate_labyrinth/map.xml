<?xml version="1.0" encoding="utf-8" standalone="no"?><map>
 <!-- マップ名 ※必須※ -->
 <name>絶望迷宮</name>
 <!-- マップ説明 ,(コンマ)で改行 -->
 <desc>様々な迷路が集まった、絶望迷宮。,この迷宮に惑わされないように…。,§4推奨ハンター数:1迷路につき1体</desc>
 <!-- マップバージョン ※必須※ -->
 <version>1.3.0</version>
<!-- 製作者  ※必須※　複数人指定可能 -->
 <authors>
    <author>creamhunter1106</author>
 </authors>
<!-- ゲーム時間 半角数字で設定/分単位/
   未設定の場合25分に設定されます -->
 <time>25</time>
<!-- サブタイトルを設定します -->
<subtitle>絶望へようこそ…</subtitle>
<!-- ゲーム開始時/途中参加時に配られる
アイテムの個数 半角数字で設定 未設定の場合5個に設定されます-->
 <start>
      <egg>8</egg>
      <bone>10</bone>
      <feather>10</feather>
 </start>
 
 <!-- 復活時に配られるアイテムの個数 
 半角数字で設定 未設定の場合5個に設定されます-->
 <rejail>
      <egg>6</egg>
      <bone>7</bone>
      <feather>7</feather>
 </rejail>
 
   <!-- 各種地点 ※必須※-->
 <location>
      <player>-3.50,4.0,4.50,45.0,0.0</player>
      <clear>0.50,18.5,0.50,90.0,0.0</clear>
      <hunter>-70.50,4.0,0.50,-90.0,0.0</hunter>
      <jail>0.50,11.5,0.50,180.0,0.0</jail>
      <rejail1>-2.50,4.0,57.50,135.0,0.0</rejail1>
      <rejail2>-2.50,4.0,-56.50,45.0,0.0</rejail2>
      <door>-68.50,4.0,1.50,-90.0,0.0</door>
      <opg>-60.50,4.0,0.50,90.0,0.0</opg>
      <!-- shopは設定しなくても可-->
      <shop>0.5,4.0,0.48,44.86,0.0</shop>
 </location>
 
<!-- ゲーム自動終了  true/falseでの記述 -->
 <auto_end>false</auto_end>
<!-- 自動復活の初期値  true/falseでの記述 -->
 <auto_rejail>false</auto_rejail>
<!-- 観戦機能の初期値  true/falseでの記述 -->
 <watch>false</watch>
<!-- 天候の初期値  半角英数字で設定 0でnormal 1でhot 2でcold  
  指定以外の数値は強制的にnormalになります -->
 <climate>0</climate>
<!-- スポーンを許可する友好モブ 複数指定可能-->
 <mob_spawn>
        <type>POLAR_BEAR</type>
 </mob_spawn>
 <!-- OPG終了時のTP  true/falseでの記述 
   trueでテレポートを行う-->
 <opg_tp>false</opg_tp>
 <!-- 復活可能回数 -->
<rejailcount>10</rejailcount>
<!-- 特殊ブロック 半角数字で設定 0不運/1盲目/2発行/3加速 -->
<spblock>1</spblock>
</map>