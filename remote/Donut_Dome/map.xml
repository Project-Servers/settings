<?xml version="1.0" encoding="utf-8" standalone="no"?>
<map>
 <!-- マップ名 ※必須※ -->
 <name>ドーナッツドーム</name>
 <!-- マップバージョン ※必須※ -->
 <version>1.0.0</version>
<!-- 製作者  ※必須※　複数人指定可能 -->
 <authors>
    <author>suzukizoku</author>
 </authors>
<!-- ゲーム時間 半角数字で設定/分単位/
   未設定の場合25分に設定されます -->
 <time>25</time>
<!-- サブタイトルを設定します -->
<subtitle>ドーナッツドーム</subtitle>
<!-- ゲーム開始時/途中参加時に配られる
アイテムの個数 半角数字で設定 未設定の場合5個に設定されます-->
 <start>
      <egg>8</egg>
      <bone>8</bone>
      <feather>8</feather>
 </start>
 
 <!-- 復活時に配られるアイテムの個数 
 半角数字で設定 未設定の場合5個に設定されます-->
 <rejail>
      <egg>6</egg>
      <bone>6</bone>
      <feather>6</feather>
 </rejail>
 
<!-- ゲーム自動終了  true/falseでの記述 -->
 <auto_end>true</auto_end>
<!-- 自動復活の初期値  true/falseでの記述 -->
 <auto_rejail>false</auto_rejail>
<!-- 観戦機能の初期値  true/falseでの記述 -->
 <watch>false</watch>
<!-- スポーンを許可する友好モブ 複数指定可能-->
 <mob_spawn>
        <type>POLAR_BEAR</type>
        <type>VILLAGER</type>
 </mob_spawn>
  <!-- OPG終了時のTP  true/falseでの記述 
   trueでテレポートを行う-->
 <opg_tp>false</opg_tp>
<location>
     <player>122.5,5,-251.5,90,0</player>
	 <hunter>116.5,5,-251.5,-90,0</hunter>
     <clear>174.5,6.5,-195.5,0,0</clear>
     <jail>187.5,7.5,-195.5,0,0</jail>
     <rejail1>156.5,17,-251.5,0,0</rejail1>
     <rejail2>156.5,5,-214.5,0,0</rejail2>
     <door>116.5,5,-251.5,-90,0</door>
     <opg>122.5,5,-251.5,90,0</opg>
</location>
</map>
