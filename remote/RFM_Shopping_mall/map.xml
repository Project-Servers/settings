<?xml version="1.0" encoding="utf-8" standalone="no"?><map>
 <!-- マップ名 ※必須※ -->
 <name>RFMショッピングモール</name>
 <!-- マップバージョン ※必須※ -->
 <version>1.0.3</version>
<!-- 製作者  ※必須※　複数人指定可能 -->
 <authors>
    <author>inazuma0415</author>
    <author>Takashing_1185</author>
 </authors>
<!-- サブタイトルを設定します -->
<subtitle>静かなショッピングモールで逃走中！</subtitle>
<!-- ゲーム開始時/途中参加時に配られる
アイテムの個数 半角数字で設定 未設定の場合5個に設定されます-->
 <start>
      <egg>12</egg>
      <bone>12</bone>
      <feather>12</feather>
 </start>
 
 <!-- 復活時に配られるアイテムの個数 
 半角数字で設定 未設定の場合5個に設定されます-->
 <rejail>
     <egg>8</egg>
      <bone>8</bone>
     <feather>8</feather>
 </rejail>
 
   <!-- 各種地点 ※必須※-->
 <location>
      <player>134.46,22.0,-135.09,-0.2,-1.04</player>
      <clear>179.54,32.0,-93.1,90.66,10.28</clear>
      <hunter>137.43,24.0,-119.3,179.75,-2.76</hunter>
      <jail>172.49,32.0,-103.79,179.36,2.5</jail>
      <rejail1>68.38,22.0,-151.46,268.6,-0.62</rejail1>
      <rejail2>125.38,30.0,-97.83,179.83,1.07</rejail2>
      <door>134.52,24.0,-119.44,179.36,-0.4</door>
      <opg>134.46,22.0,-135.09,-0.2,-1.04</opg>
<!-- shopは設定しなくても可-->
   <shop>164.3,30.0,-100.44,90.82,2.86</shop>

 </location>
<!-- ゲーム自動終了  true/falseでの記述 -->
 <auto_end>true</auto_end>
<!-- 自動復活の初期値  true/falseでの記述 -->
 <auto_rejail>false</auto_rejail>
<!-- 観戦機能の初期値  true/falseでの記述 -->
 <watch>false</watch>
<opg_tp>false</opg_tp>
 <!-- 復活可能回数 -->
<rejailcount>10</rejailcount>
<!-- 特殊ブロック 半角数字で設定 0不運/1盲目/2発行/3加速 -->
<spblock>0</spblock> 
<!-- 時間帯の初期値  半角英数字で設定 0/朝,1/昼,2/夜,  
  指定以外の数値や未設定の場合は0になります -->
 <timezone>1</timezone>
 <!-- フィールドの初期値  半角英数字で設定 
 0/平穏,1/温暖,2/寒冷,3/灼熱,4/極寒,5/絶望,6/希望,7/暴風,8/天雷,9/感染  
  指定以外の数値や未設定の場合は0になります -->
 <field>0</field>
<desc>郊外にあるショッピングモール,ご来店お待ちしております,§c推奨ハンター数2人</desc> 
<!-- 自動でナイトビジョンを設定するか-->
<auto_nightvision active="true"/>
</map>

