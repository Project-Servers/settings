<?xml version="1.0" encoding="utf-8" standalone="no"?>
<map>
 <!-- マップ名 ※必須※ -->
 <name>六本木ビルズ</name>
 <!-- マップバージョン ※必須※ -->
 <version>1.0.0</version>
<!-- 製作者  ※必須※　複数人指定可能 -->
 <authors>
    <author>suzukizoku</author>
 </authors>
<!-- ゲーム時間 半角数字で設定/分単位/
   未設定の場合25分に設定されます -->
 <time>25</time>
<!-- サブタイトルを設定します -->
<subtitle>族長の六本木ビルズ</subtitle>
<!-- ゲーム開始時/途中参加時に配られる
アイテムの個数 半角数字で設定 未設定の場合5個に設定されます-->
 <start>
      <egg>7</egg>
      <bone>7</bone>
      <feather>7</feather>
 </start>
 
 <!-- 復活時に配られるアイテムの個数 
 半角数字で設定 未設定の場合5個に設定されます-->
 <rejail>
      <egg>4</egg>
      <bone>4</bone>
      <feather>4</feather>
 </rejail>
 
<!-- ゲーム自動終了  true/falseでの記述 -->
 <auto_end>true</auto_end>
<!-- 自動復活の初期値  true/falseでの記述 -->
 <auto_rejail>false</auto_rejail>
<!-- 観戦機能の初期値  true/falseでの記述 -->
 <watch>false</watch>
<!-- スポーンを許可する友好モブ 複数指定可能-->
 <mob_spawn>
        <type>POLAR_BEAR</type>
        <type>VILLAGER</type>
 </mob_spawn>
  <!-- OPG終了時のTP  true/falseでの記述 
   trueでテレポートを行う-->
 <opg_tp>false</opg_tp>
<location>
     <player>8.5,7,8.5,0,0</player>
	 <hunter>8.5,13,26.5,180,0</hunter>
     <clear>28.5,3.5,2.5,0,0</clear>
     <jail>19.5,3.5,-4.5,0,0</jail>
     <rejail1>0.5,13,27.5,0,0</rejail1>
     <rejail2>8.5,30,-12.5,0,0</rejail2>
     <door>8.5,13,26.5,180,0</door>
     <opg>8.5,13,22.5,0,0</opg>
</location>
</map>
